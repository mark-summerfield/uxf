FileComment Str("A hierarchical alternative to some foreign key IDs.\nWe've kept Customers as-is because different invoices may be for the same\ncustomer.")
TClassBegin ttype=<Customers>
  Field Str("CID") vtype=int
  Field Str("Company") vtype=str
  Field Str("Address") vtype=str
  Field Str("Contact") vtype=str
  Field Str("Email") vtype=str
TClassEnd
TClassBegin ttype=<Invoices>
  Field Str("INUM") vtype=int
  Field Str("CID") vtype=int
  Field Str("Raised_Date") vtype=date
  Field Str("Due_Date") vtype=date
  Field Str("Paid") vtype=bool
  Field Str("Description") vtype=str
  Field Str("Items") vtype=Items
TClassEnd
TClassBegin ttype=<Items>
  Field Str("IID") vtype=int
  Field Str("Delivery_Date") vtype=date
  Field Str("Unit_Price") vtype=real
  Field Str("Quantity") vtype=int
  Field Str("Description") vtype=str
TClassEnd
ListBegin # There is a 1:M relationship between the Invoices and Items tables
  TableBegin ttype=<Customers>
    Int(50)
    Str("Best People")
    Str("123 Somewhere")
    Str("John Doe")
    Str("j@doe.com")
    Int(19)
    Str("Supersuppliers")
    ?
    Str("Jane Doe")
    Str("jane@super.com")
  TableEnd
  TableBegin ttype=<Invoices>
    Int(152)
    Int(50)
    Date(2022-01-17)
    Date(2022-02-17)
    Bool(false)
    Str("COD")
    TableBegin ttype=<Items>
      Int(1839)
      Date(2022-01-16)
      Real(29.99)
      Int(2)
      Str("Bales of hay")
      Int(1840)
      Date(2022-01-16)
      Real(5.98)
      Int(3)
      Str("Straps")
    TableEnd
    Int(153)
    Int(19)
    Date(2022-01-19)
    Date(2022-02-19)
    Bool(true)
    Str("")
    TableBegin ttype=<Items>
      Int(1620)
      Date(2022-01-19)
      Real(11.5)
      Int(1)
      Str("Washers (1-in)")
    TableEnd
  TableEnd
ListEnd
